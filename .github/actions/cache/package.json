{
  "name": "cache",
  "version": "1.0.0",
  "private": true,
  "description": "Cache newcity.json, always",
  "main": "dist/restore/index.js",
  "scripts": {
    "build": "tsc && ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronsky/newcity.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "Aaron Sky <aaronsky@skyaaron.com>",
  "license": "GPLv3",
  "dependencies": {
    "@actions/cache": "^1.0.4",
    "@actions/core": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@vercel/ncc": "^0.25.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  }
}
